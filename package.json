{
  "name": "digma-ui",
  "version": "16.7.0-alpha.2",
  "description": "Digma UI",
  "scripts": {
    "lint:eslint": "eslint --cache .",
    "lint:stylelint": "stylelint '**/*.ts'",
    "lint:knip": "knip --cache",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "fix:prettier": "prettier --write '**/*.{json,md,mjs,ts,tsx}'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "start": "webpack serve --config webpack.dev.ts --env PLATFORM=web",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate-types": "npx @rtk-query/codegen-openapi openapi-config.ts",
    "get-jaeger-ui": "node --experimental-transform-types ./scripts/get-jaeger-ui.mts",
    "update-dependencies": "node --experimental-transform-types ./scripts/update-dependencies.mts",
    "prebuild": "rimraf dist && npm run get-jaeger-ui",
    "build:dev": "npm run prebuild && webpack --progress --config webpack.dev.ts",
    "build:dev:jetbrains": "npm run build:dev -- --env PLATFORM=jetbrains",
    "build:dev:jetbrains:zip": "npm run build:dev:jetbrains -- --env ZIP=true --env ZIP_FILE_FORMAT=jetbrains",
    "build:dev:vs": "npm run build:dev -- --env PLATFORM=vs",
    "build:dev:vs:zip": "npm run build:dev:vs -- --env ZIP=true",
    "build:dev:web": "npm run build:dev -- --env PLATFORM=web",
    "build:dev:web:zip": "npm run build:dev:web -- --env ZIP=true",
    "build:prod": "npm run prebuild && webpack --progress --config webpack.prod.ts",
    "build:prod:jetbrains": "npm run build:prod -- --env PLATFORM=jetbrains",
    "build:prod:jetbrains:zip": "npm run build:prod:jetbrains -- --env ZIP=true --env ZIP_FILE_FORMAT=jetbrains",
    "build:prod:vs": "npm run build:prod -- --env PLATFORM=vs",
    "build:prod:vs:zip": "npm run build:prod:vs -- --env ZIP=true",
    "build:prod:web": "npm run build:prod -- --env PLATFORM=web",
    "build:prod:web:zip": "npm run build:prod:web -- --env ZIP=true",
    "precommit": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{mjs,ts,tsx}": [
      "eslint --cache"
    ],
    "src/**/*.ts": [
      "stylelint"
    ],
    "*.{json,mjs,md,ts,tsx}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx}": [
      "jest --bail --findRelatedTests --passWithNoTests"
    ]
  },
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.32.0",
    "@octokit/rest": "^22.0.0",
    "@rtk-query/codegen-openapi": "^2.0.0",
    "@storybook/addon-a11y": "^9.0.18",
    "@storybook/addon-designs": "^10.0.2",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.6",
    "@storybook/react-webpack5": "^9.0.18",
    "@testing-library/react": "^16.3.0",
    "@types/adm-zip": "^0.5.7",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-helmet": "^6.1.11",
    "@types/react-lottie": "^1.2.10",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/react-transition-group": "^4.4.12",
    "@types/semver": "^7.7.0",
    "@types/zip-webpack-plugin": "^3.0.6",
    "adm-zip": "^0.5.16",
    "babel-loader": "^10.0.0",
    "babel-plugin-styled-components": "^2.1.4",
    "cheerio": "^1.1.2",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "dotenv": "^17.2.1",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.0.18",
    "fs-extra": "^11.3.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "knip": "^5.62.0",
    "lint-staged": "^16.1.2",
    "postcss-styled-syntax": "^0.7.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "storybook": "^9.0.18",
    "storybook-addon-remix-react-router": "^5.0.0",
    "style-loader": "^4.0.0",
    "stylelint": "^16.22.0",
    "stylelint-config-standard": "^38.0.0",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "typescript-plugin-styled-components": "^3.0.0",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "zip-webpack-plugin": "^4.0.3"
  },
  "dependencies": {
    "@codemirror/lang-json": "^6.0.2",
    "@floating-ui/react": "^0.27.14",
    "@formkit/auto-animate": "^0.8.2",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@react-oauth/google": "^0.12.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-table": "^8.21.3",
    "@uiw/react-codemirror": "^4.24.1",
    "@xyflow/react": "^12.8.2",
    "allotment": "^1.20.4",
    "axios": "^1.11.0",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^4.1.0",
    "free-email-domains": "^1.2.20",
    "posthog-js": "^1.258.2",
    "react": "^18.3.1",
    "react-cool-dimensions": "^3.0.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^6.0.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.61.1",
    "react-lottie": "^1.2.10",
    "react-markdown": "^10.1.0",
    "react-product-fruits": "^2.2.61",
    "react-redux": "^9.2.0",
    "react-router": "~7.6.3",
    "react-syntax-highlighter": "^15.6.1",
    "react-transition-group": "^4.4.5",
    "recharts": "^2.15.3",
    "redux-remember": "^5.2.0",
    "semver": "^7.7.2",
    "squarify": "^1.1.0",
    "styled-components": "^6.1.19",
    "uuid": "^11.1.0",
    "zustand": "^4.5.5",
    "zustand-slices": "^0.3.0"
  },
  "overrides": {
    "react-syntax-highlighter": {
      "prismjs": "1.30.0"
    }
  }
}
